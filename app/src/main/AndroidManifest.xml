<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.shanejansen.mvptest">

  <application
      android:allowBackup="true"
      android:icon="@mipmap/ic_launcher"
      android:label="@string/app_name"
      android:supportsRtl="true"
      android:theme="@style/AppTheme">
    <activity android:name="com.shanejansen.mvptest.mainactivity.MainActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />
        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>

    <activity
        android:name="com.shanejansen.mvptest.secondactivity.SecondActivity"
        android:parentActivityName="com.shanejansen.mvptest.mainactivity.MainActivity" />

    <activity
        android:name="com.shanejansen.mvptest.multifragmentactivity.MultiFragmentActivity"
        android:parentActivityName="com.shanejansen.mvptest.mainactivity.MainActivity" />
  </application>

</manifest>
